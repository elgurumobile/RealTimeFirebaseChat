# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'ProjectX' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ProjectX
  #rxswift
  pod 'RxSwift',    '~> 4.0'
  pod 'RxCocoa',    '~> 4.0'
  # Firebase
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  # Facebook
  pod 'FacebookCore'
  pod 'FacebookLogin'
  # Google
  pod 'GoogleSignIn'
  #Twitter
  pod 'TwitterKit'
  # Message
  pod 'MessageKit'
end
